        "src/features/timetable/course-site-menu-dark-theme.css",
        "src/features/login-button-cursor/login-button-cursor.css"
      ],
      "js": [
        "src/classes/color-rgb.js",
        "src/classes/course.js",
        "src/features/unlock-scroll/unlock-scroll.js",
        "src/features/login-status.js",
        "src/features/timetable/course-entry.js",
        "src/features/timetable/course-site-menu.js",
        "src/features/timetable/assignments.js",
