{
  "manifest_version": 3,
  "name": "Procyon",
  "version": "1.0",
  "description": "Make PandA more accessible",
  "icons": {
    "48": "icons/procyon-48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://panda.ecs.kyoto-u.ac.jp/*"
      ],
      "css": [
        "src/features/timetable/timetable.css",
        "src/features/timetable/timetable-light-theme.css",
        "src/features/timetable/timetable-dark-theme.css",
        "src/features/timetable/course-entry.css",
        "src/features/timetable/course-entry-light-theme.css",
        "src/features/timetable/course-entry-dark-theme.css",
